<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ===== =====Base Ingredients===== ===== -->
		<RecipeDef Name="Diet_CanCarnMealBase" ParentName="Diet_CanMealBaseDef" Abstract="True">
		<fixedIngredientFilter>
			<thingDefs>
				<li>Meat_Human</li>
			</thingDefs>
			<categories>
				<li>MeatRaw</li>
				<li>AnimalProductRaw</li>
			</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>AnimalProductRaw</li>
			</categories>
			<disallowedCategories>
				<li>EggsFertilized</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>InsectJelly</li>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
	</RecipeDef>
	<!-- ===== =====Single Meals===== ===== -->
	<!-- ===== FINE ===== -->
	<RecipeDef Name="Diet_CanCarnFineMeal" ParentName="Diet_CanCarnMealBase">
		<defName>Diet_CanCarnFineMeal</defName>
		<label>Fine Cannibal Carnivore Meal</label>
		<description>Cook a fine carnivore meal containing at least 2 pieces of human meat.</description>
		<jobString>Cooking a fine cannibal carnivore meal</jobString>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Meat_Human</li>
					</thingDefs>
				</filter>
				<count>0.10</count>
			</li>
			<li>
				<filter>
				  <categories>
					<li>MeatRaw</li>
					<li>AnimalProductRaw</li>
				  </categories>
				</filter>
				<count>0.65</count>
			</li>
		</ingredients>
		<products>
			<Diet_CanCarnFineMeal>1</Diet_CanCarnFineMeal>
		</products>
		<workAmount>450</workAmount>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
	</RecipeDef>
