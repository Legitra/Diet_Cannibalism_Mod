			<li>
				<filter>
					<thingDefs>
						<li>Meat_Human</li>
					</thingDefs>
				</filter>
				<count>0.1</count>
			</li>
			<li>
				<filter>
				  <categories>
					<li>MeatRaw</li>
					<li>AnimalProductRaw</li>
				  </categories>
				</filter>
				<count>0.24</count>
			</li>
			<li>
				<filter>
				  <categories>
					<li>PlantFoodRaw</li>
				  </categories>
				</filter>
				<count>0.25</count>
			</li>
		</ingredients>
		<products>
			<Diet_CanFineMeal>1</Diet_CanFineMeal>
		</products>
		<workAmount>450</workAmount>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
	</RecipeDef>
